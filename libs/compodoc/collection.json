{
  "$schema": "../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "factory": "./src/schematics/ng-add/schematic",
      "schema": "./src/schematics/ng-add/schema.json",
      "description": "Install compodoc plugin."
    },
    "config": {
      "factory": "./src/schematics/config/schematic",
      "schema": "./src/schematics/config/schema.json",
      "description": "Configure compodoc target for a project.",
      "aliases": ["configuration"]
    },
    "workspace-docs-html": {
      "factory": "./src/schematics/workspace-docs-html-generator/schematic",
      "schema": "./src/schematics/workspace-docs-html-generator/schema.json",
      "description": "Generate a HTML website which holds all libs/apps documentations in a clear overview."
    }
  }
}
